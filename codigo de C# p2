using System;

class Program {
  public static void Main (string[] args) {
    double[] nota = new double[3];
    double md=0;
    for(int i=0; i<3; i++){
    Console.Write("Digite a {0} nota:", i+1);
      nota[i] = double.Parse(Console.ReadLine());
      }
    for(int i=0; i<3; i++){
      md=md+nota[i]/3;
    }
    Console.WriteLine("a media Ã©: {0:f2}", md);
    if (md>=7){
      Console.Write("aprovado");
    }
    else
    if(md<=7){
      Console.Write("reprovado");
    }
    }
}
